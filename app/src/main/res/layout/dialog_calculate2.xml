<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView_dialog_placeTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="26dp"
        android:text="거리          " />

    <EditText
        android:id="@+id/editText_dialog_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:gravity="center"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="16dp"
        android:layout_toEndOf="@+id/textView_dialog_placeTitle"
        android:layout_toRightOf="@+id/textView_dialog_placeTitle"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:singleLine="true"
        android:scrollbars="horizontal"
        android:text="" />

    <TextView
        android:id="@+id/textView_dialog_placeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="76dp"
        android:text="안테나 각도" />

    <EditText
        android:id="@+id/editText_dialog_antennaAngle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="56dp"
        android:gravity="center"
        android:layout_toEndOf="@+id/textView_dialog_placeTitle"
        android:layout_toRightOf="@+id/textView_dialog_placeTitle"
        android:ems="10"
        android:inputType="numberDecimal|numberSigned"
        android:singleLine="true"
        android:scrollbars="horizontal"
        android:text="" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="127dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/x33"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:paddingLeft="0dp"
            android:paddingRight="0dp"
            android:backgroundTint="@color/tumyeongsaeg"
            android:textColor="@color/darkgray"
            android:stateListAnimator="@null"
            android:textAllCaps="false"
            android:text="x33" />

        <Button
            android:id="@+id/p29"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:paddingLeft="0dp"
            android:paddingRight="0dp"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/tumyeongsaeg"
            android:textColor="@color/darkgray"
            android:stateListAnimator="@null"
            android:textAllCaps="false"
            android:text="P(29dBi)" />

        <Button
            android:id="@+id/p30"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:paddingLeft="0dp"
            android:paddingRight="0dp"
            android:backgroundTint="@color/tumyeongsaeg"
            android:stateListAnimator="@null"
            android:text="P(30dBi)"
            android:textAllCaps="false"
            android:textColor="@color/darkgray" />

    </LinearLayout>


    <Button
        android:id="@+id/button_dialog_calculateBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="87dp"
        android:layout_marginTop="145dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/blue"
        android:textColor="@color/white"
        android:text="계산" />

    <Button
        android:id="@+id/button_delete_Btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="187dp"
        android:layout_marginTop="145dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@color/blue"
        android:text="취소"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="210dp"
        android:layout_marginBottom="22dp"
        android:textStyle="bold"
        android:text="반경 범위" />

    <TextView
        android:id="@+id/distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="57dp"
        android:gravity="center"
        android:layout_marginBottom="22dp"
        android:textStyle="bold"
        android:layout_marginTop="210dp"
        android:layout_toEndOf="@+id/textView_dialog_placeTitle"
        android:layout_toRightOf="@+id/textView_dialog_placeTitle"
        android:ems="10"
        android:inputType="text"
        android:singleLine="true"
        android:scrollbars="horizontal"
        android:text="" />



</RelativeLayout>